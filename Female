        "# SEX 데이터 안에 F이면 1 M이면 0으로 값 대체\n",
        "# 'NaN' 값을 0으로 대체\n",
        "\n",
        "# raw['SEX2'] = np.where(raw['SEX2'] == b'F', 1, 0)\n",
        "# raw['SEX3'] = np.where(raw['SEX3'] == b'F', 1, 0)\n",
        "# raw['SEX4'] = np.where(raw['SEX4'] == b'F', 1, 0)\n",
        "\n",
        "raw['SEX1'] = raw['SEX1'].apply(lambda x: 1 if x == \"b'F'\" else (0 if x == \"b'M'\" else x)).fillna(0)\n",
        "raw['SEX2'] = raw['SEX2'].apply(lambda x: 1 if x == \"b'F'\" else (0 if x == \"b'M'\" else x)).fillna(0)\n",
        "raw['SEX3'] = raw['SEX3'].apply(lambda x: 1 if x == \"b'F'\" else (0 if x == \"b'M'\" else x)).fillna(0)\n",
        "raw['SEX4'] = raw['SEX4'].apply(lambda x: 1 if x == \"b'F'\" else (0 if x == \"b'M'\" else x)).fillna(0)"
